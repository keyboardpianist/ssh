<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="entity.Homework" table="Homework">
        <!-- 联合主键 -->
        <composite-id name="homeworkPK" class="entity.HomeworkPK">
            <key-property name="ho_as_id" column="ho_as_id"  type="string"/>
            <key-property name="ho_gr_id" column="ho_gr_id"  type="string"/>
        </composite-id>


        <property name="ho_time" column="ho_time" type="date" />
        <property name="ho_path" column="ho_path" />
        <property name="ho_name"  column="ho_name" />
        <property name="ho_us_id" column="ho_us_id" />
    </class>
</hibernate-mapping>
