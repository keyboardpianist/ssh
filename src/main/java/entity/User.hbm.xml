<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-mapping>
    <class name="entity.User" table="User">
        <id name="id" column="us_id" />

        <property name="name" column="us_name" />
        <property name="classNo" column="us_class" />
        <property name="password" column="us_password" />
        <property name="role" column="us_role" />
    </class>
</hibernate-mapping>