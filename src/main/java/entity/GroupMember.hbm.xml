<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="entity.GroupMember" table="GroupMember">
        <!-- 联合主键 -->
        <composite-id name="groupMemberPK" class="entity.GroupMemberPK">
            <key-property name="gm_gr_id" column="gm_gr_id"  type="string"/>
            <key-property name="gm_st_id" column="gm_st_id"  type="string"/>
        </composite-id>

        <many-to-one name="groupMemberPK" class="entity.Group" lazy="false">
            <column name="gm_gr_id"></column>
        </many-to-one>

        <many-to-one name="groupMemberPK" class="entity.Group" lazy="false">
            <column name="gm_st_id"></column>
        </many-to-one>



        <property name="valid" column="ho_valid" />

    </class>
</hibernate-mapping>
